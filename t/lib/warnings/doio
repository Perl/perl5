# doio.c [Perl_do_open9]
use warnings 'io' ;
open(F, '|'."$^X -e 1|");
close(F);
no warnings 'io' ;
open(G, '|'."$^X -e 1|");
close(G);
EXPECT
Can't open bidirectional pipe at - line 3.
